<% include partials/header %>

<h1>The Posts Page</h1>

<p>Using for loop</p>
<% for(var i = 0; i < posts.length ; i++){ %>
    <li>
        <%= posts[i].title %> - <strong>Author <%= posts[i].author %> </strong>
    </li>
<% } %>

<!--using forEach-->

<p>Using forEach loop</p>
<% posts.forEach(function(post){ %>
    <li>
        <%= post.title %> - <strong>Author <%= post.author %> </strong>
    </li>
<% }) %>

<% include partials/footer %>